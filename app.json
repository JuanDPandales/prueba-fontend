{
  "expo": {
    "name": "Prueba Frontend RN",
    "slug": "prueba-frontend-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D7F9F1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pruebafrontendrn.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#D7F9F1"
      },
      "package": "com.pruebafrontendrn.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client"
    ],
    "scheme": "prueba-frontend-rn",
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}